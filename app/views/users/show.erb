<div class="container">
  <h2> Welcome <%= @user.name %>! </h2>
  <dl>
    <dt> Name: </dt>
    <dd> <%= @user.name %></dd>
  </dl>
  <dl>
    <dt> Email: </dt>
    <dd> <%= @user.email %></dd>
  </dl>
  <dl>
    <dt> Urls: </dt>
    <dd> <ul>
      <% @urls.each do |url| %>
      <li>
       <%= url.original%> <a href="http://<%= url.original %>"><%= url.url_alias %></a>
      </li>
      <% end %>
    </ul></dd>
  </dl>
</div>
